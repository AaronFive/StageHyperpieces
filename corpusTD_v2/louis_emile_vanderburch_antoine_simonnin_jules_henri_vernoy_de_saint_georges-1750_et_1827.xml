            <l n="3" xml:id="l3">MONSIEUR JADIS</l>
            <l n="4" xml:id="l4">MONSIEUR DUJOUR</l>
      </sp>
        <sp who="strong>" xml:id="0-2">
            <speaker><strong><em>Personnages du Premier Tableau</em></strong></speaker>
            <l n="5" xml:id="l5">LE MARQUIS D'ABLIMAR</l>
            <l n="6" xml:id="l6">LE PETIT DUC</l>
            <l n="7" xml:id="l7">LE CHEVALIER</l>
